    <TableRow>
        <TextView android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="上出位" android:layout_weight="1.0" />
        <TextView android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="顶出位" android:layout_weight="1.0" />
