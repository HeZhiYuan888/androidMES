<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout 
		android:orientation="vertical" 
        android:background="@null" 
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent"
		xmlns:android="http://schemas.android.com/apk/res/android">

    <ScrollView 
    android:scrollbarThumbVertical="@drawable/scrollbar_handle_vertical" 
    android:background="#ffecf0f1" 
    android:layout_width="fill_parent" 
    android:layout_height="0.0px" 
    android:layout_weight="1.0">
    
        <LinearLayout 
        android:orientation="vertical" 
        android:layout_width="fill_parent" 
        android:layout_height="fill_parent" >

            <TextView 
            android:textColor="#ff414141" 
            android:layout_width="wrap_content" 
            android:layout_height="wrap_content" 
            android:layout_marginLeft="14.0dip" 
            android:layout_marginTop="7.0dip" 
            android:layout_marginBottom="7.0dip" 
            android:textStyle="bold"
            android:text="充型压力曲线" />

            <tool.MyChartView
                android:id="@+id/menulist"
                android:layout_width="match_parent"
                android:layout_height="300dip"
                android:layout_margin="5dip"
                android:background="@drawable/shape_bg" />
            
    <TableLayout 
	android:background="@drawable/shape_bg" 
	android:paddingLeft="6.0dip" 
	android:paddingTop="5.0dip" 
	android:paddingRight="6.0dip" 
	android:paddingBottom="5.0dip" 
	android:layout_width="fill_parent" 
	android:layout_height="wrap_content" 
	android:layout_marginLeft="5.0dip" 
	android:layout_marginTop="0.0dip" 
	android:layout_marginRight="5.0dip" 
	android:layout_marginBottom="9.0dip">
    
    
    <TableRow>
        <TextView android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:layout_weight="1.0" />
        <TextView android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="保压时间" android:layout_weight="1.0" />
        <TextView android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="冷却时间" android:layout_weight="1.0" />
        <TextView android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="充型压力" android:layout_weight="1.0" />
    </TableRow>
    <TableRow>
        <TextView  android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="实际" android:layout_weight="1.0" />
        <TextView android:id="@+id/lowPressTimesReal" android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:layout_weight="1.0" />
        <TextView android:id="@+id/CoolingTimesReal" android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:layout_weight="1.0" />
        <TextView android:id="@+id/fillTimesReal" android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:layout_weight="1.0" />
    </TableRow>
    <TableRow>
        <TextView  android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="设置" android:layout_weight="1.0" />
        <TextView android:id="@+id/lowPressTimesOption" android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:layout_weight="1.0" />
        <TextView android:id="@+id/CoolingTimesOption" android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:layout_weight="1.0" />
        <TextView android:id="@+id/fillTimesOption" android:textSize="14.0sp" android:textColor="@color/traffic_txt" android:padding="2.0dip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="" android:layout_weight="1.0" />
    </TableRow>
    
</TableLayout>
            
            <TextView 
            android:textSize="12.0sp" 
            android:textColor="#ff414141" 
            android:layout_width="wrap_content" 
            android:layout_height="wrap_content" 
            android:layout_marginLeft="10.0dip" 
            android:layout_marginTop="2.0dip" 
            android:layout_marginBottom="10.0dip" 
            android:text="*以上数据是充型实时数据，该实时值的获取请在wifi条件下进行，土豪随意" />
            
            
            <TextView 
            android:textSize="12.0sp" 
            android:textColor="#ff414141" 
            android:layout_width="wrap_content" 
            android:layout_height="wrap_content" 
            android:layout_marginLeft="10.0dip" 
            android:layout_marginTop="2.0dip" 
            android:layout_marginBottom="10.0dip" 
            android:gravity="center"
            android:layout_gravity="center"
            android:text="\@CopyRight 浙江工业大学" />
            
         
            

        </LinearLayout>


    </ScrollView>
</LinearLayout>